import tkinter
import time


class TestTkinter(tkinter.Frame):
    def __init__(self):
        super().__init__()
        self.label = tkinter.Label(self, text="Hello, World!")
        self.label.pack()


TestTkinter = TestTkinter()
subscribe.views.Setting().InsertNotebook(TestTkinter, "Test Tkinter Notebook")
time.sleep(5)
subscribe.views.Setting().DeleteNotebook(TestTkinter)

